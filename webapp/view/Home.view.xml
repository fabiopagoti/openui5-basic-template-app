<mvc:View
	controllerName="sap.ui.demo.basicTemplate.controller.Home"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		id="page"
		title="{i18n>title}">
		<content>
			<List items="{/people}" >
				<StandardListItem title="{name}" />
			</List>
		</content>
	</Page>
</mvc:View>